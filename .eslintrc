{
  "extends": "airbnb-base",
  "env": {
    "node": true
  },
  "ecmaFeatures": {
    "es5": true,
    "es6": true
  },
  "rules": {
    "semi": 0,
    "strict": 0,
    "global-require": 0,
    "prefer-arrow-callback": 0,
    "no-console": 0,
    "padded-blocks": 0,
    "import/no-dynamic-require": 0,
    "no-empty": ["error", { "allowEmptyCatch": true }],
    "no-unused-expressions": 0,
    "eol-last": 0,
    "space-before-function-paren": 0,
    "prefer-template": 0,
    "object-shorthand": 0,
    "quotes": 0,
    "vars-on-top": 0,
    "comma-dangle": 0,
    "func-names": 0,
    "prefer-const": 0,
    "arrow-body-style": 0,
    "import/no-unresolved": 0,
    "no-param-reassign": 0,
    "no-return-assign": 0,
    "max-len": 0,
    "consistent-return": 0,
    "no-loop-func": 0
  }
}
